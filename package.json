{
  "name": "web",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "concurrently --kill-others \"npm run web:run\" \"npm run api:run\"",
    "lint": "tslint \"src/**/*.ts\" --project src/tsconfig.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "build": "npm run web:build && npm run api:build",
    "test": "npm run api:test && npm run web:test",
    "web:build": "ng build --prod --sm=false --aot=true --output-path=dist/public",
    "web:run": "ng serve  --proxy-config proxy.conf.json",
    "web:test": "ng test --watch=false",
    "api:build": "tsc -p ./api",
    "api:run": "nodemon api/bin/www.ts",
    "api:test": "npm run api:build && ava --verbose ./dist/test/**/*.js",
    "postinstall": "npm run build"
  },
  "private": true,
  "engines": {
    "node": "^6.9.4",
    "npm": "3.10.10"
  },
  "dependencies": {
    "@angular/common": "^2.4.4",
    "@angular/compiler": "^2.4.4",
    "@angular/core": "^2.4.4",
    "@angular/forms": "^2.4.4",
    "@angular/http": "^2.4.4",
    "@angular/platform-browser": "^2.4.4",
    "@angular/platform-browser-dynamic": "^2.4.4",
    "@angular/router": "^3.4.4",
    "@angular/compiler-cli": "^2.4.4",
    "@types/body-parser": "0.0.33",
    "@types/compression": "0.0.33",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.0.35",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.42",
    "@types/supertest": "^2.0.0",
    "angular-cli": "1.0.0-beta.26",
    "ava": "^0.17.0",
    "body-parser": "^1.16.0",
    "compression": "^1.6.2",
    "core-js": "^2.4.1",
    "express": "^4.14.0",
    "path": "^0.12.7",
    "rxjs": "^5.0.1",
    "serve-favicon": "^2.3.2",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "codelyzer": "~2.0.0-beta.1",
    "concurrently": "^3.1.0",
    "cors": "^2.8.1",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "nodemon": "^1.11.0",
    "protractor": "~4.0.13",
    "supertest": "^2.0.1",
    "ts-node": "1.2.1",
    "tslint": "^4.3.0",
    "typescript": "~2.0.3"
  }
}